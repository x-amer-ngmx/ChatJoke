@model ChatJoke.Models.RemModel
@{
    ViewBag.Title = "title";
    Layout = "../_Layout/_fullInterface.cshtml";
}


@using (Html.BeginForm("Registration", null, FormMethod.Post, new { }))
{
    @Html.AntiForgeryToken()

    @Html.TextBoxFor(m => m.UserName, new { maxlength = 50, tabindex = 1 })

    @Html.PasswordFor(m => m.Ticket, new { maxlength = 50, tabindex = 2 })

    <input class="_enter_st" type="submit" value="Зарегистрироваться" tabindex=3 />
}